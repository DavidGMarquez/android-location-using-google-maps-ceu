<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:padding="5dip" android:id="@+id/formalta" android:background="@drawable/interior2"
	android:orientation="vertical">



	<TextView android:id="@+id/label" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:background="@android:drawable/editbox_background"
		android:textStyle="bold" android:typeface="serif"

		android:text="Introducir datos Vehiculo:                      ejem(4888 mercedes E300..)" />


	<EditText android:id="@+id/id_vehiculo" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:background="@android:drawable/editbox_background"
		android:singleLine="true" android:layout_below="@id/label" />



	<Button android:id="@+id/btn_showMessage" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_below="@id/id_vehiculo"
		android:layout_alignParentRight="true" android:layout_marginLeft="10px"
		android:text="Guardar" />

</RelativeLayout>